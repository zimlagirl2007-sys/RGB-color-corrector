// This code allows you to have thhe RGB turn on and be a specific colour by giving each LEDs a specific value
// This is a analog output program
void setup() {
  // put your setup code here, to run once:
 Serial.begin(9600); 

  // define the pinmodes as an output 

  pinMode(13, OUTPUT);  

  pinMode(12, OUTPUT);  

  pinMode(11, OUTPUT);   
}

void loop() {
// define each LED to a specific value
  analogWrite(13,238); 

  analogWrite(12,159); 

  analogWrite(11,18); 

  delay(1);  // delay in between reads for stability

  // put your main code here, to run repeatedly:

}
